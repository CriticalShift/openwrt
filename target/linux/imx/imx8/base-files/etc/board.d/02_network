. /lib/functions/uci-defaults.sh
. /lib/functions.sh
. /lib/functions/system.sh

board_config_update

board=$(board_name)

case "$board" in
compulab,ucm-imx8m-plus)
	ucidef_set_interface_lan "eth0 eth1 mwiphy0-ap0"
	ucidef_set_interface_wan "wwan0"
	;;
esac

board_config_flush

exit 0
